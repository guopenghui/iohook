{
  "name": "@tkomde/iohook",
  "version": "1.1.2",
  "description": "Node.js global keyboard and mouse hook",
  "author": "tkomda",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkomde/iohook.git"
  },
  "scripts": {
    "install": "node install.js",
    "build": "node build.js --upload=false",
    "build:ci": "node build.js --all --openssl_fips=''",
    "build:print": "node -e 'require(\"./helpers\").printManualBuildParams()'"
  },
  "keywords": [
    "hook",
    "electron",
    "nw.js",
    "listener",
    "mousemove",
    "keypress",
    "keyup",
    "global keypress",
    "shortcut"
  ],
  "lint-staged": {
    "examples/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "docs/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "nan": "^2.22.2",
    "nugget": "^2.0.1",
    "pump": "^1.0.3",
    "rc": "^1.2.8",
  "tar-fs": "^1.16.3",
  "node-gyp": "^11.2.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^24.0.0",
    "electron": "^37.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^11.1.1",
    "minimist": "^1.2.5",
    "node-abi": "^3.45.0",
    "node-gyp": "^11.2.0",
    "prebuild": "^13.0.1",
    "prettier": "^3.6.0",
    "tar": "^7.4.3"
  },
  "supportedTargets": [
    [ "electron", "27.0.0", "118" ],
    [ "electron", "28.0.0", "119" ],
    [ "electron", "29.0.0", "121" ],
    [ "electron", "30.0.0", "123" ],
    [ "electron", "31.0.0", "125" ],
    [ "electron", "32.0.0", "128" ],
    [ "electron", "33.0.0", "130" ],
    [ "electron", "34.0.0", "132" ],
    [ "electron", "35.0.0", "133" ],
    [ "electron", "36.0.0", "135"],
    [ "electron", "37.0.0", "136"],
    [ "node", "20.0.0", "115" ],
    [ "node", "22.0.0", "127" ],
    [ "node", "24.0.0", "137" ]
  ],
  "iohook": {
    "targets": [
      "node-137",
      "electron-136"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64"
    ]
  }
}
