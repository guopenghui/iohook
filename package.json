{
  "name": "@tkomdef/iohook",
  "version": "1.1.1",
  "description": "Node.js global keyboard and mouse hook",
  "author": "tkomda",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkomde/iohook.git"
  },
  "scripts": {
    "build": "node build.js --upload=false",
    "build:ci": "node build.js --all --openssl_fips=''",
    "build:print": "node -e 'require(\"./helpers\").printManualBuildParams()'"
  },
  "keywords": [
    "hook",
    "electron",
    "nw.js",
    "listener",
    "mousemove",
    "keypress",
    "keyup",
    "global keypress",
    "shortcut"
  ],
  "lint-staged": {
    "examples/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "docs/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "nan": "^2.22.2",
    "nugget": "^2.0.1",
    "pump": "^1.0.3",
    "rc": "^1.2.8",
    "tar-fs": "^1.16.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@types/node": "^7.0.62",
    "electron": "^25.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^11.1.1",
    "minimist": "^1.2.5",
    "node-abi": "^3.45.0",
    "node-gyp": "^9.4.0",
    "prebuild": "^11.0.4",
    "prettier": "^3.0.0",
    "tar": "^6.1.15"
  },
  "supportedTargets": [
    [
      "electron",
      "31.0.0",
      "125"
    ],
    [
      "electron",
      "29.0.0",
      "121"
    ],
    [
      "electron",
      "27.0.0",
      "118"
    ],
    [
      "node",
      "22.0.0",
      "127"
    ]
  ]
}
